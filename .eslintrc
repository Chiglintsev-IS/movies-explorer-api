{
    "env": {
      "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "rules": {
      "no-console": ["error", { "allow": ["error"] }],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_id"
          ]
        }
      ]
    }
  }